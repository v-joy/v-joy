<div id="article-detail-wrapper">
    <div class="clearfix">
        <div class="article-head clearfix">
            <h1 class="article-title">
                {{item.title}}
            </h1>
            <span class="article-time f-right">
                发表于 {{item.createTime}}
            </span>
        </div>
        <hr>
        <div class="article-content">
            {{item.content}}
        </div>
    </div>
</div>

